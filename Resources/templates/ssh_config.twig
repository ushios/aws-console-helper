{% if NoStrictHostKeyChecking %}
StrictHostKeyChecking no
{% endif %}
{% for host in config %}
Host {{ host.Host }}_{{ host.hostname }}
    hostname {{ host.hostname }}
{% if host.User %}
    User {{ host.User }}
{% endif %}
{% if host.IdentityFile %}
    IdentityFile {{ host.IdentityFile }}
{% endif %}
{% if host.Port %}
    Port {{ host.Port }}
{% endif %}

{% endfor %}